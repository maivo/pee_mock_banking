<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pee.mockbanking" >

    <application
        android:name="pee.mockbanking.AppSession"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pee.mockbanking.MainActivity"
            android:label="@string/activity_main_title" >
        </activity>
        <activity
            android:name="pee.mockbanking.SecondActivity"
            android:label="@string/activity_second_title">
        </activity>
        <activity
            android:name="pee.mockbanking.LoginActivity"
            android:label="@string/activity_login_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
